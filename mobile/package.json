{
  "name": "piggy-bank-of-happiness",
  "description": "零時小學校提案：小草書屋的幸福存摺",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest --watchAll",
    "lint": "eslint .",
    "build:ios": "node -r esm publish-check.js prd && node build-version.js ios && expo build:ios -t archive",
    "build:ios:dev": "node -r esm publish-check.js develop && node build-version.js ios && expo build:ios -t archive --release-channel develop",
    "build:android": "node -r esm publish-check.js prd && node build-version.js android && expo build:android",
    "build:android:dev": "node -r esm publish-check.js develop && node build-version.js android && expo build:android --release-channel develop",
    "upload:ios": "expo upload:ios",
    "upload:android": "expo upload:android --latest",
    "publish": "node -r esm publish-check.js prd && expo publish",
    "publish:dev": "node -r esm publish-check.js develop && node build-version.js && expo publish --release-channel develop"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^10.1.0",
    "@react-native-community/masked-view": "0.1.6",
    "@react-native-community/netinfo": "^5.9.2",
    "@react-native-community/picker": "^1.6.5",
    "@react-navigation/bottom-tabs": "^5.3.1",
    "@react-navigation/native": "^5.2.1",
    "@react-navigation/stack": "^5.2.16",
    "await-to-js": "^2.1.1",
    "aws-amplify": "^3.0.17",
    "aws-amplify-react-native": "^4.2.0",
    "date-time-format-timezone": "^1.0.22",
    "expo": "~37.0.9",
    "expo-asset": "~8.1.0",
    "expo-camera": "^8.3.1",
    "expo-constants": "~9.0.0",
    "expo-font": "~8.1.0",
    "expo-image-manipulator": "^8.2.1",
    "expo-image-picker": "~8.1.0",
    "expo-linking": "^1.0.1",
    "expo-splash-screen": "^0.2.3",
    "expo-updates": "^0.2.14",
    "expo-web-browser": "~8.2.0",
    "intl": "^1.2.5",
    "lodash.debounce": "^4.0.8",
    "logkitty": "^0.7.1",
    "moment": "^2.27.0",
    "native-base": "2.13.8",
    "react": "~16.9.0",
    "react-dom": "~16.9.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-37.0.1.tar.gz",
    "react-native-elements": "^2.3.2",
    "react-native-extra-dimensions-android": "^1.2.5",
    "react-native-floating-action": "^1.21.0",
    "react-native-gesture-handler": "~1.6.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.1",
    "react-native-loading-spinner-overlay": "^1.1.0",
    "react-native-material-dropdown": "^0.11.1",
    "react-native-modal": "^11.5.6",
    "react-native-numeric-input": "^1.8.3",
    "react-native-paper": "^3.10.1",
    "react-native-popup-menu": "^0.15.9",
    "react-native-safe-area-context": "0.7.3",
    "react-native-screens": "~2.2.0",
    "react-native-web": "~0.11.7",
    "react-native-webview": "^11.0.0",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "babel-eslint": "^10.1.0",
    "babel-preset-expo": "~8.1.0",
    "eslint": "^7.2.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-native": "^3.8.1",
    "esm": "^3.2.25",
    "jest-expo": "~37.0.0",
    "metro-config": "^0.59.0"
  },
  "private": true
}
